from marshmallow import fields, Schema

class UserInfoSchema(Schema):
	# rollno = fields.Str()
	name = fields.Str()
	email = fields.Email()
	dob = fields.DateTime()
	mobno = fields.Int()


info_schema = UserInfoSchema()

